
#Aside

  %ul.menu
    -@children.each do |child|
      -if content = child.contents.find_by(lang: locale)
        %li
          =link_to child.slug do
            %span= content.title

  = raw module_to(@modules, 'left')

#MainCol
  .text.clearFix
    = raw module_to(@modules, 'top')
    %h1= @content.title
    %p= raw @content.body
    %p= @page.parent_path
    = raw module_to(@modules, 'partners')
